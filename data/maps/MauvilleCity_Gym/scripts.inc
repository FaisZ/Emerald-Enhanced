	compare VAR_0x8008, TRAINER_REL_BRENDAN
	goto_if_eq MVG_GymBattleIntroBrendan
	compare VAR_0x8008, TRAINER_REL_LANA
	goto_if_eq MVG_GymBattleIntroLana
