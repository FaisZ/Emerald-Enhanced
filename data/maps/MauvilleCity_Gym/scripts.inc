	goto_if_eq MVG_GymBattleIntroLanette
	compare VAR_0x8008, TRAINER_REL_COURTNEY_2
	goto_if_eq MVG_GymBattleIntroCourtney
	compare VAR_0x8008, TRAINER_REL_NURSE
